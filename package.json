{
	"name": "fracpane",
	"version": "0.1.3",
	"private": false,
	"type": "module",
	"exports": {
		"./package.json": "./package.json",
		".": "./dist/index.js"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"dev": "vite dev",
		"debug": "NODE_OPTIONS=\"--inspect\" vite dev",
		"build": "vite build",
		"package": "rm -rf dist && svelte-kit sync && svelte-package",
		"package-watch": "svelte-package --watch",
		"release": "pnpm package && rm ./dist/package.json && changeset publish",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test": "vitest"
	},
	"dependencies": {
		"@neodrag/svelte": "^1.2.3",
		"@neodrag/vanilla": "^1.0.1",
		"@nanostores/persistent": "^0.7.0",
		"@tweakpane/plugin-essentials": "^0.1.5",
		"nanostores": "^0.7.0",
		"rxjs": "^7.5.7",
		"tweakpane": "^3.1.0"
	},
	"devDependencies": {
		"@changesets/cli": "^2.25.0",
		"@sveltejs/adapter-auto": "1.0.0-next.80",
		"@sveltejs/kit": "1.0.0-next.511",
		"@sveltejs/package": "1.0.0-next.5",
		"@svitejs/changesets-changelog-github-compact": "^0.1.1",
		"@tweakpane/core": "^1.1.0",
		"@types/node": "^18.8.3",
		"@types/postcss-preset-env": "^7.7.0",
		"@types/prismjs": "^1.26.0",
		"@typescript-eslint/eslint-plugin": "^5.39.0",
		"@typescript-eslint/parser": "^5.39.0",
		"autoprefixer": "^10.4.12",
		"copy": "^0.3.2",
		"cssnano": "^5.1.13",
		"dotenv": "^16.0.3",
		"eslint": "^8.24.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"fracpane": "^0.1.3",
		"fractils": "^3.0.2",
		"greset": "^2.0.0",
		"mdsvex": "^0.10.6",
		"nanoid": "^4.0.0",
		"nodemon": "^2.0.20",
		"npm-run-all": "^4.1.5",
		"postcss": "^8.4.17",
		"postcss-load-config": "^4.0.1",
		"postcss-preset-env": "7.8.2",
		"prettier": "2.7.1",
		"prettier-plugin-svelte": "^2.7.1",
		"prismjs": "^1.29.0",
		"pug": "^3.0.2",
		"rehype-autolink-headings": "^6.1.1",
		"rehype-slug": "^5.0.1",
		"remark-abbr": "^1.4.1",
		"sass": "^1.55.0",
		"svelte": "^3.50.1",
		"svelte-check": "^2.9.1",
		"svelte-preprocess": "^4.10.7",
		"svelte2tsx": "^0.5.19",
		"tslib": "^2.4.0",
		"typedoc": "^0.23.15",
		"typescript": "^4.8.4",
		"vite": "^3.1.6",
		"vitest": "^0.23.4"
	},
	"packageManager": "pnpm@7.0.0",
	"engines": {
		"node": ">= 16",
		"npm": "forbidden, use pnpm",
		"pnpm": "^7.0.0"
	}
}